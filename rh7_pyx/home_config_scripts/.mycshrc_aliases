#! /bin/csh
# vi: syntax=tcsh

alias ll 'ls -alF'
alias la 'ls -A'
alias lt 'ls -lt'
alias ltr 'ls -ltr'

## PYX STRANGER THINGS
alias whichproj 'head -3 $CMD/projets/projdef; grep -i "\!*" $CMD/projets/projdef; tail -1 $CMD/projets/projdef;echo $comment;echo $CMD/projets/projdef;echo $comment'
alias pyxca52 "source $HOME/pyx_utils/set_pyx_proj pyxca52"
