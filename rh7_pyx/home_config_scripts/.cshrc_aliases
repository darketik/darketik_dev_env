#! /bin/csh
# vi: syntax=tcsh

alias ll 'ls -alF'
alias la 'ls -A'
alias lt 'ls -lt'
alias ltr 'ls -ltr'

## PYX STRANGER THINGS
### projdef: it is a txt file !!
alias whichproj 'head -3 $HOME/pyx_utils/projdef; grep -i "\!*" $HOME/pyx_utils/projdef; tail -1 $HOME/pyx_utils/projdef;echo $comment;echo $HOME/pyx_utils/projdef;echo $comment'

alias pyxca52_FR_img140 "source $HOME/pyx_utils/set_pyx_proj pyxca52 FR_img140"
alias pyxca52_FR_img140_DELIV "source $HOME/pyx_utils/set_pyx_proj pyxca52 FR_img140_DELIV"
alias pyxca46a0 "source $HOME/pyx_utils/set_pyx_proj pyxca46a0"
alias pkvl pyxca52_FR_img140
alias ouranos pyxca46a0
